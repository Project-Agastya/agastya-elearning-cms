{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "Student"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "FirstName": {
      "type": "string",
      "required": true
    },
    "LastName": {
      "type": "string"
    },
    "Photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Email": {
      "type": "string"
    },
    "School": {
      "type": "string"
    },
    "City": {
      "type": "string"
    },
    "Courses": {
      "collection": "course"
    }
  }
}
